               </del>,<lb/>
		  Acili<del>us Cleobu<supplied reason="undefined">lus <expan><abbr>v</abbr><ex>ir</ex></expan>
                     <expan><abbr>c</abbr><ex>larissimus</ex></expan>
                  </supplied>
               </del>
