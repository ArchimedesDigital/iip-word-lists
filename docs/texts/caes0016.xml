		  ac <expan><abbr>feli</abbr><ex>cissimo</ex></expan>
               <del>ciss <choice>
                     <abbr>
                        <supplied reason="undefined">i</supplied>ndu<supplied reason="undefined">lgent</supplied>
                     </abbr>
